import java.util.Scanner;

public class ChangeCoin {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int coin, oman, man, ochun, chun, obak, bak, sip, il = 0;
		System.out.println("돈 입력해바>>");
		coin = in.nextInt();
		
		oman = coin/50000;
		man = (coin-(oman * 50000))/10000; 
		ochun = (coin-(man * 10000))/5000;
		chun = (coin-(ochun * 5000))/1000;
		obak = (coin-(chun * 50000))/500;
		bak = (coin-(obak * 50000))/100;
		sip = (coin-(bak * 50000))/10;
		il = coin-(sip);
		
		
		if(coin > 50000) {
			System.out.println("오만원권" +  + "매");
			System.out.println("만원권" +  + "매");
			System.out.println("오천원권" +  + "매");
			System.out.println("천원권" +  + "매");
			System.out.println("오백원" +  + "매");
			System.out.println("백원" +  + "매");
			System.out.println("십원" +  + "매");
			System.out.println("일원" +  + "매");
		}
		else if(coin > 10000) {
			System.out.println("만원권" +  + "매");
			System.out.println("오천원권" +  + "매");
			System.out.println("천원권" +  + "매");
			System.out.println("오백원" +  + "매");
			System.out.println("백원" +  + "매");
			System.out.println("십원" +  + "매");
			System.out.println("일원" +  + "매");
		}else if(coin > 5000) {
			System.out.println("오천원권" +  + "매");
			System.out.println("천원권" +  + "매");
			System.out.println("오백원" +  + "매");
			System.out.println("백원" +  + "매");
			System.out.println("십원" +  + "매");
			System.out.println("일원" +  + "매");
		}else if(coin > 1000) {
			System.out.println("천원권" +  + "매");
			System.out.println("오백원" +  + "매");
			System.out.println("백원" +  + "매");
			System.out.println("십원" + + "매");
			System.out.println("일원" + coin%10 + "매");
		}else if(coin > 500) {
			System.out.println("오백원" + coin/500 + "매");
			System.out.println("백원" + coin/100 + "매");
			System.out.println("십원" + coin/10 + "매");
			System.out.println("일원" + coin%10 + "매");
		}else if(coin > 100) {
			System.out.println("백원" + coin/100 + "매");
			System.out.println("십원" + coin/10 + "매");
			System.out.println("일원" + coin%10 + "매");
		}else if(coin > 10) {
			System.out.println("십원" + coin/10 + "매");
			System.out.println("일원" + coin%10 + "매");
		}else {
			System.out.println("오만원권" + coin/50000 + "매");
			System.out.println("만원권" + coin/10000 + "매");
			System.out.println("오천원권" + coin/5000 + "매");
			System.out.println("천원권" + coin/1000 + "매");
			System.out.println("오백원" + coin/500 + "매");
			System.out.println("백원" + coin/100 + "매");
			System.out.println("십원" + coin/10 + "매");
			System.out.println("일원" + coin%10 + "매");
		}
	
	}

}
