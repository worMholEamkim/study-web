import java.util.Scanner;

public class ClapClap {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int hund = 0;
		int tenPosi = hund / 10;
		int onePosi = hund % 10;
		
		System.out.println("1~99 사이의 정수를 입력해봐>>");
		hund = in.nextInt();
		if(tenPosi % 3 == 0) {
			if(onePosi % 3 == 0)
				System.out.println("박수짝짝쓰");
			else
				System.out.println("박수짝쓰");}
		else if(onePosi % 3 == 0)
			System.out.println("박수짝스");
		}
	
}
